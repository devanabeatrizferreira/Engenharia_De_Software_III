public class Fila {
	
	private static int quant_impressoes;
	private static Fila instancia;
	
	private Fila() {
		Fila fila = new Fila();
		
	}
	
	public static Fila getInstancia() {
		if (instancia == null) {
			instancia = new Fila();
		}
		return instancia;
	}

	public static int getQuant_impressoes() {
		return quant_impressoes;
	}

	public static void setQuant_impressoes(int quant_impressoes) {
		Fila.quant_impressoes = quant_impressoes;
	}
	
}
